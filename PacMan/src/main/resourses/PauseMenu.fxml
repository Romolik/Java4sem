<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="400.0" maxWidth="400.0" minHeight="400.0" minWidth="400.0" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.ru.nsu.shchiptsov.Pacman.PauseMenuController">
    <children>
        <Pane maxHeight="400.0" maxWidth="400.0" minHeight="400.0" minWidth="400.0" prefHeight="400.0" prefWidth="400.0">
            <children>
                <ImageView fitHeight="400.0" fitWidth="400.0">
                    <image>
                        <Image url="@BackgroundForGame.png" />
                    </image>
                </ImageView>
            <Label layoutX="118.0" prefHeight="65.0" prefWidth="164.0" text="Pause" textAlignment="RIGHT">
               <font>
                  <Font name="AppleMyungjo Regular" size="62.0" />
               </font>
            </Label>
            <VBox layoutX="100.0" layoutY="136.0" maxHeight="200.0" maxWidth="200.0" minHeight="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="resumeButton" maxHeight="100.0" maxWidth="200.0" minHeight="100.0" minWidth="200.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: transparent;" text="Resume">
                     <font>
                        <Font name="AppleMyungjo Regular" size="38.0" />
                     </font>
                  </Button>
                  <Button fx:id="exitButton" maxHeight="100.0" maxWidth="200.0" minHeight="100.0" minWidth="200.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: transparent;" text="Exit">
                     <font>
                        <Font name="AppleMyungjo Regular" size="38.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            </children>
        </Pane>
    </children>
</AnchorPane>
